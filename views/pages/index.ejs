<!DOCTYPE html>
<html>
<head>
  <title>Results</title>
</head>

<body>

  <div>
      <%
      var reslut = 0;
      var number1 = parseInt(num1);
      var number2 = parseInt(num2);
      if(operand == "add")
      {
          reslut = number1 + number2;
      }
      else if(operand == "sub")
      {
          reslut = number1 - number2;
      }
      else if(operand == "mul")
      {
          reslut = number1 * number2;
      }
      else if(operand == "div")
      {
          reslut = number1 / number2;
      }
       %>

      <h1><%= reslut %></h1>

  </div>

</body>
</html>
